// Operations
// primitive datatype

console.log("1"+2)  //12
console.log(1+"2") //12
console.log("a" + 2 + 2) // 122
console.log(1+ "2" + 5) //125

console.log(1+true); //2
console.log(+true) //1 
console.log(true+9) //1 

